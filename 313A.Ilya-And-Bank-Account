import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception{
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if(n >= 0)
            System.out.println(n);
        else{
            int last = n/10;
            int second = n/100 * 10 + n%10;
            int max  = Math.max(last, second);
            System.out.println(max);
        }
    }
}
